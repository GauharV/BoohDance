public class BoohbahDanceTest {
    public static void main(String[] args) {
        Boohbah boohbah1= new Boohbah("Zing Zing Zingbah", "Spins and Sparkles");
        Boohbah boohbah2= new Boohbah("Humbah", "Wiggle Wiggle");
        Boohbah boohbah3= new Boohbah("Jumbah", "Twirl of Joy");

        Boohbah[] theboohbahs= {boohbah1,boohbah2,boohbah3};
        DanceRoutine routine = new DanceRoutine(theboohbahs);

        System.out.println("Orignal Dance Routine:");
        String originalRoutine =routine.buildRoutine();
        System.out.println(originalRoutine);

        System.out.println("Modified Routine");
        String modifiedRoutine =routine.modifyroutine();
        System.out.println(modifiedRoutine);
    }
}
